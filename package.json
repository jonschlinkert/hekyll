{
  "name": "hekyll",
  "description": "Convert Jekyll themes to use handlebars instead of liquid.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/hekyll",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/hekyll",
  "bugs": {
    "url": "https://github.com/jonschlinkert/hekyll/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "delete": "^1.0.1",
    "gulp-liquid-to-handlebars": "^0.1.0",
    "is-valid-app": "^0.3.0",
    "through2": "^2.0.3",
    "vinyl-fs": "^2.4.4"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.2"
  },
  "keywords": [
    "hekyll"
  ],
  "lintDeps": {
    "lock": {
      "options": {
        "cheerio": "0.22.0"
      }
    }
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
