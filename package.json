{
  "name": "hekyll",
  "description": "Migrate Jekyll (gh-pages) themes to use handlebars instead of liquid.",
  "version": "2.0.0",
  "homepage": "https://github.com/jonschlinkert/hekyll",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/hekyll",
  "bugs": {
    "url": "https://github.com/jonschlinkert/hekyll/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "assemble-core": "^0.31.0",
    "delete": "^1.1.0",
    "file-is-binary": "^1.0.0",
    "gulp-liquid-to-handlebars": "^1.0.1",
    "plugin-error": "^0.1.2",
    "through2": "^2.0.3",
    "vinyl-fs": "^2.4.4"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.0"
  },
  "keywords": [
    "assemble",
    "blog",
    "convert",
    "converter",
    "gh-pages",
    "github-pages",
    "gulp",
    "handlebars",
    "hekyll",
    "jekyll",
    "liquid",
    "migrate",
    "migrator",
    "pages",
    "site",
    "templates",
    "theme",
    "website"
  ],
  "lintDeps": {
    "lock": {
      "options": {
        "cheerio": "0.22.0"
      }
    }
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "gulp-liquid-to-handlebars",
        "liquid-to-handlebars"
      ]
    },
    "reflinks": [
      "assemble",
      "gulp",
      "handlebars-helpers",
      "template-helpers"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
